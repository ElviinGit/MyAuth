<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <ul>
    <% userList.forEach(user => { %>
        <li><strong>Name:</strong> <%= user.new_username %>, <strong>Password:</strong> <%= user.new_password %></li>
    <% }); %>
    </ul>
    <br>
    <ul>
    <% blogList.forEach(all_blogs =>{ %>
        <li><strong>Tittle-----</strong><%= all_blogs.title %>, <strong>Content-----</strong> <%= all_blogs.content %>, <strong>Author----- </strong><%= all_blogs.author %></li>
    <% }) %> 
    </ul>
</body>
</html>